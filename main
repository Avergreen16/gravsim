import pygame

Width, Height = 900, 500
Window = pygame.diplsay.set_mode((Width, Heigth))

Black = (0, 0, 0)

def gameloop():
  Window.fill(Black)
  
  pygame.display.update()

def main():
  run = True
  
  while run:
    for event in pygame.event.get():
      if event == pygame.QUIT:
        run = False
        pygame.quit()

if __name__ = "__main__":
  main()
